doctype html
html
    head
        block scripts
            // if lt IE 9
                <script src='/static/lib/html5shiv.js'></script>
            title=title
            link(rel="stylesheet",href="/static/css/material-icons.css")
            link(rel='stylesheet',href='/static/css/materialize.min.css',media="screen,projection")
            meta(name="viewport",content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0")
            block apm
            script(src='/static/lib/jquery.min.js')
            script(src='/static/js/materialize.min.js')
            script(src="/static/lib/sha1.js")
            link(rel="stylesheet",href="/static/lib/sweetalert.css")
            //link(rel="stylesheet",href="/static/lib/sweetalert-google.css")
            script(src="/static/lib/sweetalert.min.js")
            include ../mixin/footer
        include color
        include fwStyle
        block style
    body
        block content
        if(!without_footer)
            +footer


